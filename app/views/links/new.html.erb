<div class="page_title">
  New Link
</div>

<div class = "page_content">
  <div class="form">
    <%= form_for([@user, @user.links.build]) do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.label :url %>
      <%= f.text_field :url %>
      <%= f.submit "Create" %>
    <% end %>
  </div>
</div>
